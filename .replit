modules = ["bash", "python-3.11"]
run = "node index.js"

[nix]
channel = "stable-24_05"
packages = ["cacert", "cargo", "file", "freetype", "glibcLocales", "lcms2", "libGL", "libiconv", "libimagequant", "libjpeg", "libmediainfo", "libtiff", "libuv", "libwebp", "libxcrypt", "openjpeg", "openssl", "rustc", "tcl", "tk", "xcodebuild", "zlib"]

[deployment]
run = ["sh", "-c", "node index.js"]

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
